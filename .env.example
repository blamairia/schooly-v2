APP_NAME=Schooly
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# Logging
LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug
LOG_SLACK_WEBHOOK_URL=

# Database
DB_CONNECTION=mysql
DATABASE_URL=
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=schooly
DB_USERNAME=root
DB_PASSWORD=
DB_SOCKET=
DB_FOREIGN_KEYS=true
MYSQL_ATTR_SSL_CA=

# Cache / Session / Queue / Redis
CACHE_DRIVER=file
CACHE_PREFIX=
MEMCACHED_PERSISTENT_ID=
MEMCACHED_USERNAME=
MEMCACHED_PASSWORD=
MEMCACHED_HOST=127.0.0.1
MEMCACHED_PORT=11211
REDIS_CLIENT=phpredis
REDIS_CLUSTER=redis
REDIS_PREFIX=
REDIS_URL=
REDIS_HOST=127.0.0.1
REDIS_USERNAME=
REDIS_PASSWORD=
REDIS_PORT=6379
REDIS_DB=0
REDIS_CACHE_DB=1

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_CONNECTION=
SESSION_STORE=
SESSION_DOMAIN=
SESSION_SECURE_COOKIE=false

QUEUE_CONNECTION=sync
QUEUE_FAILED_DRIVER=database-uuids
SQS_PREFIX=
SQS_QUEUE=default
SQS_SUFFIX=

# Filesystems / S3
FILESYSTEM_DISK=local
FILAMENT_FILESYSTEM_DISK=public
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_URL=
AWS_ENDPOINT=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Mail
MAIL_MAILER=smtp
MAIL_URL=
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=null
MAIL_EHLO_DOMAIN=
MAIL_SENDMAIL_PATH="/usr/sbin/sendmail -bs -i"
MAIL_LOG_CHANNEL=
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Broadcast / Realtime
BROADCAST_DRIVER=log
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# Logging / Observability
LOG_STDERR_FORMATTER=
LOG_PAPERTRAIL_HANDLER=
PAPERTRAIL_URL=
PAPERTRAIL_PORT=

# Security / Auth
SANCTUM_STATEFUL_DOMAINS=
SANCTUM_TOKEN_PREFIX=
BCRYPT_ROUNDS=12

# Debug / Dev tools
DEBUGBAR_ENABLED=null
DEBUGBAR_OPEN_STORAGE=
DEBUGBAR_EDITOR=
IGNITION_EDITOR=phpstorm
DEBUGBAR_REMOTE_SITES_PATH=
DEBUGBAR_LOCAL_SITES_PATH=
DEBUGBAR_THEME=auto

# Printing / External services
PRINTING_DRIVER=printnode
PRINT_NODE_API_KEY=
CUPS_SERVER_IP=
CUPS_SERVER_USERNAME=
CUPS_SERVER_PASSWORD=
CUPS_SERVER_PORT=631

# Backup / App-specific
BACKUP_ARCHIVE_PASSWORD=

# Misc
DYNAMODB_CACHE_TABLE=cache
DYNAMODB_ENDPOINT=

# Optional / commented helpers
#DB_ENCRYPT=
#DB_TRUST_SERVER_CERTIFICATE=
